############################################################## Card fields
card style:
	############################# Background stuff
	border color:
		left:	0
		top:	0
		width:	750
		height:	1050
		z index: 4
		mask: {"silverborder"+(if is_legend() then "_crown" else "") + ".png" }
		visible: {card.border_color != rgb(0,0,0)}

	card color:
		left:	0
		top:	0
		width:	750
		height:	1050
		z index: 0
		render style: image
		popup style: in place
		image: 
			script:
				if is_spree() then spree_background()
				else if is_devoid() then devoid_background()
				else if is_colorless() and not is_fullart() then "vcard.png"
				else if is_ub() then ub_background()
				else if is_circuit() then circuit_background()
				else if is_snow() then snow_background()
				else card_background()
		mask: { if is_fullart() then "frame_mask_fullart.png" else if is_mutate() then "frame_mask_mutate.png" else "frame_mask.png" }
	card color 2:
		left: { if is_adventure() then 36 else  50 }
		top:  { if is_adventure() then 654 else  657 }
		width: { if is_adventure() then 340 else  650 }
		height: { if is_adventure() then 315 else  125 }
		render style: image
		popup style: in place
		image: { if is_adventure() then leftpage_background() else prototype_background()}
		z index: 1
		visible: {is_adventure() or is_prototype()}
	############################# Name line
	name:
		left: { 64 }
		top: { nametop_shift() }
		right:  { 682 - max(30, card_style.casting_cost.content_width) }
		height: {46 - 0.5*shrink_name()}
		alignment: { if use_phyrexian_font() then "top shrink-overflow" else "bottom shrink-overflow"}
		padding bottom: 0
		z index: 5
		font:
			name: {nametype_font_choose()}
			size: { if set.card_language == "日本語" then 26 - shrink_name() else if set.card_language != "简体中文" then 32 - shrink_name() else 34 - shrink_name()}
			color: black
	name 3:
		left: { if set.card_language == "日本語" then 64 else 0}
		top: { if set.card_language == "日本語" then 59 else 0}
		right:  { 682 - max(30, card_style.casting_cost.content_width) }
		height: { if set.card_language == "日本語" then 10 else 0}
		alignment: {"bottom shrink-overflow"}
		padding bottom: 0
		z index: 5
		font:
			name: { "A-OTF Kaimin Tsuki Std B"}
			size: {10 - shrink_name()}
			color: black
	casting cost:
		right: 692
		top: 57
		width: { max(60, card_style.casting_cost.content_width) + 10 }
		height: 46
		alignment: middle right
		font:
			name: MPlantin
			size: 30
		symbol font:
			name: magic-mana-large
			size: 30
			alignment: middle right
		always symbol: true
		z index: 2
		padding top: 0
	alias:
		left: 108
		top: 116
		width: 536
		height: 30
		z index: 5
		visible: { has_alias() }
		font:
			name: { if set.card_language != "简体中文" then "MPlantin-Italic" else ch_flavor_font()  }
			size: 20
			scale down to: 6
			color: white
		alignment: middle center
		
	name 2:
		left: {if is_adventure() then 64 else 0 }
		top: {(if is_adventure() then if set.card_language != "简体中文" then 666 else 660 else 0)}
		right: { if is_adventure() then (356 - max(40, card_style.casting_cost_2.content_width)) else 0 }
		height: { if is_adventure() then (40 - 0.5*shrink_adventure_name()) else 0 }
		alignment: { if use_phyrexian_font() then "middle shrink-overflow" else "bottom shrink-overflow"}
		z index: 2
		font:
			name: { if use_phyrexian_font() then "Phi_horizontal_gbrsh_3.0" else if set.card_language != "简体中文" then "Beleren Bold" else "华康魏碑 Std W7" }
			size: {26 - shrink_adventure_name() }
			color: white
		
	casting cost 2:
		right: {if is_adventure() then 362 else if is_prototype() then 688 else 0 }
		top: {if is_adventure() then 659 else if is_prototype() then 664 else 0 }
		width: { if is_adventure() or is_prototype() then max(20, card_style.casting_cost_2.content_width) + 10 else 0}
		height: 50
		alignment: middle right
		symbol font:
			name: magic-mana-large
			size: {if is_adventure() then 25 else if is_prototype() then 26 else 0 }
			alignment: middle right
		always symbol: true
		z index: 4
		padding top: 0

	############################# Image
	image:
		left:	{ if is_devoid() or is_colorless() then 30 else if is_mutate() or is_fullart() then 47 else 56 }
		top:	{ if is_devoid() then 107 else if is_colorless() then 30 else 117 }
		width:	{ if is_devoid() or is_colorless() then 691 else if is_mutate() or is_fullart() then 656 else 638 }
		height:	{ if is_devoid() then 960 else if is_colorless() then 939 else if is_fullart() then 853 else if is_mutate() then 674 else 467 }
		z index: -1
		default: {default_image(card.card_color)}
		# mask: image_mask.png
	############################# Card type   # - (if use_phyrexian_font() then shrink_type()/2 else 0)
	indicator:
		left: 62
		top: 604
		width: 34
		height: 34
		z index: 1
		render style: image
		visible: { has_identity() }
		image: { card_identity() }
	type:
		left: { if has_identity() then "102" else "62" }
		top: { typetop_shift() }
		width: { (if has_identity() then "580" else "610") - max(44,card_style.rarity.content_width) }
		height: { 40 - shrink_type() }
		alignment: {if set.card_language == "日本語" then "middle shrink-overflow" else "top shrink-overflow"}
		z index: 2
		padding top: 2
